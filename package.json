  "repository": {
    "type": "git",
    "url": "https://github.com/BayBreezy/nuxt-internal-socket"
  },
  "version": "1.0.1",
  "license": "MIT",
  "type": "module",
  "author": {
    "email": "behon.baker@yahoo.com",
